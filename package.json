    "@babel/template": "^7.2.2",
    "@babel/traverse": "^7.6.2",
    "@babel/types": "^7.3.4",
    "chalk": "^4.0.0",
    "common-tags": "^1.8.0",
    "css-loader": "^3.0.0",
    "errno": "^0.1.7",
    "fast-levenshtein": "^2.0.6",
    "fs-extra": "^9.0.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.15",
    "magic-string": "^0.25.7",
    "memory-fs": "^0.5.0",
    "postcss-atroot": "^0.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.1",
    "resolve": "^1.11.1"
  },
  "peerDependencies": {
    "webpack": ">=2"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.3.4",
