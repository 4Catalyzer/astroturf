{
  "name": "astroturf",
  "version": "1.0.0-beta.17",
  "description": "Extract and process inline CSS literals in JavaScript files",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "lint": "tsc -p . --noEmit && eslint src",
    "test": "yarn test:ts && jest",
    "test:ts": "yarn eslint types --ext .tsx --ext .ts",
    "tdd": "jest --watch",
    "prepublishOnly": "yarn build",
    "copy:types": "cpy lib/runtime/*.d.ts lib/cjs && cpy lib/runtime/*.d.ts lib/esm",
    "build:pick": "cherry-pick --cwd=lib --input-dir=../src/runtime --cjs-dir=cjs --esm-dir=esm",
    "build:runtime": "4c build src/runtime --no-copy-files --no-types",
    "build:tools": "4c build -d lib --no-esm -- --ignore \"**/runtime/**\"",
    "build": "yarn build:tools && yarn build:runtime && yarn build:pick && yarn copy:types",
    "release": "4c release",
    "start": "yarn --cwd ./example start"
  },
  "publishConfig": {
    "directory": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/astroturf.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(__file_snapshots__/)*.js": [
      "eslint --fix",
      "yarn 4c format"
    ],
    "README.md": [
      "doctoc"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ],
    "watchPathIgnorePatterns": [
      "build",
      "__file_snapshots__"
    ]
  },
  "author": {
    "name": "4Catalyzer"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4Catalyzer/astroturf/issues"
  },
  "homepage": "https://github.com/4Catalyzer/astroturf#readme",
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/core": "^7.12.3",
    "@babel/generator": "^7.12.1",
    "@babel/helper-module-imports": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/template": "^7.10.4",
    "@babel/traverse": "^7.12.1",
    "@babel/types": "^7.12.1",
    "@types/common-tags": "^1.8.0",
    "@types/fs-extra": "^9.0.2",
    "@types/loader-utils": "^2.0.1",
    "@types/resolve": "^1.17.1",
    "@types/webpack": "^4.41.22",
    "chalk": "^4.1.0",
    "common-tags": "^1.8.0",
    "cosmiconfig": "^7.0.0",
    "css-loader": "^5.0.0",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^9.0.1",
    "json5": "^2.1.3",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.20",
    "magic-string": "^0.25.7",
    "postcss": "^8.1.2",
    "postcss-nested": "^5.0.1",
    "resolve": "^1.18.1",
    "webpack-virtual-modules": "^0.3.2"
  },
  "peerDependencies": {
    "webpack": ">=2"
  },
  "devDependencies": {
    "@4c/babel-preset": "^8.0.1",
    "@4c/cli": "^2.1.11",
    "@4c/tsconfig": "^0.3.1",
    "@babel/cli": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/lodash": "^4.14.162",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "autoprefixer": "^10.0.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "cherry-pick": "^0.5.0",
    "cpy-cli": "^3.1.1",
    "doctoc": "^1.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-4catalyzer-react": "^1.0.12",
    "eslint-config-4catalyzer-typescript": "^3.0.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-ts-expect": "^1.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "jest-file-snapshot": "^0.3.6",
    "lint-staged": "^10.4.2",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^1.1.0",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "strip-ansi": "^6.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "readme": "ERROR: No README data found!",
  "_id": "astroturf@0.10.3-beta.0"
}
